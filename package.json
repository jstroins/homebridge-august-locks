{
  "displayName": "Homebridge August Smart Locks",
  "name": "homebridge-august-locks-jstroins",
  "version": "1.0.0",
  "description": "Updated August Smart Lock plugin for Homebridge with fixes and JSON parsing improvements.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jstroins/homebridge-august-locks"
  },
  "bugs": {
    "url": "https://github.com/jstroins/homebridge-august-locks/issues"
  },
  "engines": {
    "homebridge": ">=1.3.0",
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "mocha -r ts-node/register src/*.spec.ts",
    "test:watch": "mocha -r ts-node/register src/*.spec.ts --watch --watch-files src/**/*.ts"
  },
  "keywords": [
    "homebridge-plugin",
    "homebridge-platform-plugin",
    "august",
    "smartlock",
    "homebridge"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.4",
    "eslint": "^8.0.1",
    "homebridge": "^1.3.5",
    "homebridge-config-ui-x": "^4.41.5",
    "mocha": "^9.1.3",
    "nock": "^13.2.1",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "prepublish": "tsc",
  "author": "Jakub Stroinski",
  "homepage": "https://github.com/jstroins/homebridge-august-locks#readme",
  "peerDependencies": {
    "homebridge": ">=1.3.0"
  }
}